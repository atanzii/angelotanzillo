                           <script src="./assets/js/script.js" type="text/javascript"></script>
                           <script src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"></script> <script src="./assets/js/swiper.js"></script>
                           <script>
                              const navbar = document.querySelector('.navbar');
                              window.onscroll = () => {
                                 if (window.scrollY > 50) {
                                    navbar.classList.add('nav-active');
                                 } else {
                                    navbar.classList.remove('nav-active');
                                 }
                              };
                           </script>
                       <script>
                           $(document).ready(function() {
                               var $navbar = $(".navbar");
                               var $menuButton = $(".menu-button");
                               var isBlurred = true;
                               var delay = 100; // Initial delay is 0.1 seconds
                           
                               function toggleBlur() {
                                   if (isBlurred) {
                                       $navbar.removeClass("nav-blur");
                                       isBlurred = false;
                                   } else {
                                       $navbar.addClass("nav-blur");
                                       isBlurred = true;
                                   }
                               }
                           
                               $navbar.addClass("nav-blur");
                           
                               $menuButton.click(function() {
                                   setTimeout(function() {
                                       toggleBlur();
                                       delay = (delay === 100) ? 300 : 100; // Toggle between 0.1s and 0.3s delay
                                   }, delay);
                               });
                           });
                           </script>
